/* components/ai-tab-bar/ai-tab-bar.wxss */
.ai-tab-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 50px;
  background-color: #ffffff;
  border-top: 1px solid #eeeeee;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 999;
}

.ai-tab-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  flex: 1;
  height: 100%;
}

.ai-tab-icon {
  width: 24px;
  height: 24px;
  margin-bottom: 2px;
}

.ai-tab-text {
  font-size: 12px;
  color: #999999;
}
